# API Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_WS_URL=ws://localhost:8000

# Environment
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_AUTO_TRADING=false
REACT_APP_ENABLE_ADVANCED_CHARTS=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_RISK_MANAGEMENT=true

# Application Settings
REACT_APP_VERSION=1.0.0
REACT_APP_TITLE=Kalshi Probability Analysis Agent
REACT_APP_DESCRIPTION=Sophisticated probability analysis agent for Kalshi prediction markets

# Debug Settings
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=debug

# Performance Settings
REACT_APP_WEBSOCKET_TIMEOUT=20000
REACT_APP_API_TIMEOUT=30000
REACT_APP_CACHE_DURATION=300000

# Chart Settings
REACT_APP_DEFAULT_CHART_THEME=light
REACT_APP_CHART_ANIMATION_DURATION=1000

# Security Settings
REACT_APP_ENABLE_CSP=true
REACT_APP_MAX_FILE_SIZE=10485760